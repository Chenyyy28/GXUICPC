{"ast":null,"code":"import axios from 'axios';\nimport { ElMessage } from 'element-plus';\nimport { ref, onCreated, reactive } from 'vue';\nexport default {\n  setup() {\n    let isCollapse = ref(true);\n    let asideWidth = ref(70);\n    let asideState = reactive({\n      routes: []\n    });\n    function close() {\n      // isCollapse = false;\n      asideState.routes;\n      console.log(asideWidth);\n    }\n    function open() {\n      // isCollapse = true;\n      asideWidth.value = 200;\n    }\n    function dynamicRouter() {\n      // this.$store.commit('setDynamicRouter', JSON.stringify(lists));\n      return new Promise(() => {\n        axios.get(\"/url/route\").then(res => res.data).then(data => {\n          const route = JSON.stringify(data);\n          const lists = JSON.parse(route).data;\n          asideState.routes = lists;\n          console.log(asideState.routes);\n        }).catch(e => {\n          console.log(e);\n          ElMessage.error(\"菜单请求错误\");\n        });\n      });\n    }\n    return {\n      isCollapse,\n      asideWidth,\n      asideState,\n      open,\n      close,\n      dynamicRouter\n    };\n  },\n  created: async function () {\n    await this.dynamicRouter();\n    // console.log(\"created: \" + this.asideState.routes);\n\n    // console.log(\"created: \" + this.routes);\n    // location.reload();\n  }\n};","map":{"version":3,"names":["axios","ElMessage","ref","onCreated","reactive","setup","isCollapse","asideWidth","asideState","routes","close","console","log","open","value","dynamicRouter","Promise","get","then","res","data","route","JSON","stringify","lists","parse","catch","e","error","created"],"sources":["C:\\Users\\18725\\Desktop\\集训队管理系统开发\\manage\\front\\manage\\src\\components\\Aside.vue"],"sourcesContent":["<template>\r\n\r\n    <el-aside :style=\"{ width: asideWidth + 'px' }\" style=\"overflow: hidden;\" class=\"aside\">\r\n        <el-radio-group v-model=\"isCollapse\" style=\"margin-bottom: 10px;width: 70px; overflow: hidden;\">\r\n            <el-radio-button @click=\"open\" v-show=\"isCollapse\" :value=\"false\">\r\n                <el-icon>\r\n                    <Open />\r\n                </el-icon>\r\n            </el-radio-button>\r\n            <el-radio-button @click=\"close\" v-show=\"!isCollapse\" :value=\"true\">\r\n                <el-icon>\r\n                    <TurnOff />\r\n                </el-icon>\r\n            </el-radio-button>\r\n        </el-radio-group>\r\n        <el-menu default-active=\"1\" active-text-color=\"#409EFF\" :collapse=\"isCollapse\" @open=\"open\" @close=\"close\"\r\n            :collapse-transition='false' style=\"width: 100%;height: 100%;border:0;\">\r\n            <!-- <p>YEs</p> -->\r\n            <template v-for=\"item in asideState.routes\" :key=\"item.id\">\r\n                <router-link :to=\"item.link\">\r\n                    <el-menu-item :index=\"item.id + ''\">\r\n                        <el-icon>\r\n                            <component :is=\"item.icon\" />\r\n                        </el-icon>\r\n                        <template #title>{{ item.name }}</template>\r\n                    </el-menu-item>\r\n                </router-link>\r\n\r\n            </template>\r\n        </el-menu>\r\n    </el-aside>\r\n</template>\r\n<script>\r\nimport axios from 'axios';\r\nimport { ElMessage } from 'element-plus';\r\nimport { ref, onCreated, reactive } from 'vue'\r\nexport default {\r\n    setup() {\r\n        let isCollapse = ref(true);\r\n        let asideWidth = ref(70);\r\n        let asideState = reactive({\r\n            routes: []\r\n        });\r\n        function close() {\r\n            // isCollapse = false;\r\n            asideState.routes;\r\n            console.log(asideWidth)\r\n        }\r\n        function open() {\r\n            // isCollapse = true;\r\n            asideWidth.value = 200\r\n        }\r\n        function dynamicRouter() {\r\n            // this.$store.commit('setDynamicRouter', JSON.stringify(lists));\r\n            return new Promise(() => {\r\n                axios.get(\"/url/route\").then(res => res.data).then(data => {\r\n                    const route = JSON.stringify(data);\r\n                    const lists = JSON.parse(route).data;\r\n                    asideState.routes = lists;\r\n                    console.log(asideState.routes);\r\n\r\n                }).catch(e => {\r\n                    console.log(e);\r\n                    ElMessage.error(\"菜单请求错误\");\r\n                })\r\n            })\r\n        }\r\n        return {\r\n            isCollapse,\r\n            asideWidth,\r\n            asideState,\r\n            open,\r\n            close,\r\n            dynamicRouter\r\n        }\r\n    },\r\n    created: async function () {\r\n        await this.dynamicRouter()\r\n        // console.log(\"created: \" + this.asideState.routes);\r\n\r\n        // console.log(\"created: \" + this.routes);\r\n        // location.reload();\r\n    }\r\n}\r\n</script>\r\n<style>\r\n.aside {\r\n    /* background-color: #a1a3a6; */\r\n}\r\n\r\n/* .el-menu-vertical-demo:not(.el-menu--collapse) {\r\n    width: 260px;\r\n    height: 100%;\r\n\r\n} */\r\n/* .el-menu-item.is-active {\r\n  background-color: #409eff!important;\r\n} */\r\n</style>"],"mappings":"AAiCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,cAAc;AACxC,SAASC,GAAG,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAI;AAC7C,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,IAAIC,UAAS,GAAIJ,GAAG,CAAC,IAAI,CAAC;IAC1B,IAAIK,UAAS,GAAIL,GAAG,CAAC,EAAE,CAAC;IACxB,IAAIM,UAAS,GAAIJ,QAAQ,CAAC;MACtBK,MAAM,EAAE;IACZ,CAAC,CAAC;IACF,SAASC,KAAKA,CAAA,EAAG;MACb;MACAF,UAAU,CAACC,MAAM;MACjBE,OAAO,CAACC,GAAG,CAACL,UAAU;IAC1B;IACA,SAASM,IAAIA,CAAA,EAAG;MACZ;MACAN,UAAU,CAACO,KAAI,GAAI,GAAE;IACzB;IACA,SAASC,aAAaA,CAAA,EAAG;MACrB;MACA,OAAO,IAAIC,OAAO,CAAC,MAAM;QACrBhB,KAAK,CAACiB,GAAG,CAAC,YAAY,CAAC,CAACC,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,CAAC,CAACF,IAAI,CAACE,IAAG,IAAK;UACvD,MAAMC,KAAI,GAAIC,IAAI,CAACC,SAAS,CAACH,IAAI,CAAC;UAClC,MAAMI,KAAI,GAAIF,IAAI,CAACG,