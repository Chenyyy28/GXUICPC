{"ast":null,"code":"import axios from 'axios';\nimport { ElMessage } from 'element-plus';\nimport moment from 'moment';\nimport { reactive, ref } from 'vue';\nimport { Search } from '@element-plus/icons-vue';\nexport default {\n  setup() {\n    let state = reactive({\n      tableData: []\n    });\n    const currentPage = ref(1);\n    const pageSize = ref(1);\n    const total = ref(1);\n    const input3 = ref('');\n    const select = ref('');\n    function dateFormat(row, column) {\n      var date = row[column.property];\n      if (date == undefined) {\n        return \"\";\n      }\n      return moment(date).format('YYYY-MM-DD HH:mm:ss');\n    }\n    function handleSizeChange(val) {\n      console.log(`${val} items per page`);\n      pageSize.value = val;\n      getUserLists();\n      // location.reload();\n    }\n    function handleCurrentChange(val) {\n      console.log(`current page: ${val}`);\n      currentPage.value = val;\n      getUserLists();\n      // location.reload();\n    }\n    const getUserLists = function () {\n      axios.get(\"/url/user\", {\n        params: {\n          currentPage: currentPage.value,\n          pageSize: pageSize.value\n        }\n      }).then(res => res.data).then(res => res.data).then(data => {\n        console.log(data);\n        state.tableData = data.list;\n        total.value = data.total;\n      }).catch(err => {\n        console.log(err);\n        ElMessage.error(\"用户列表获取失败\");\n      });\n    };\n    return {\n      state,\n      dateFormat,\n      currentPage,\n      pageSize,\n      input3,\n      select,\n      total,\n      handleSizeChange,\n      handleCurrentChange,\n      getUserLists\n    };\n  },\n  beforeCreate() {\n    this.getUserLists();\n  }\n};","map":{"version":3,"names":["axios","ElMessage","moment","reactive","ref","Search","setup","state","tableData","currentPage","pageSize","total","input3","select","dateFormat","row","column","date","property","undefined","format","handleSizeChange","val","console",