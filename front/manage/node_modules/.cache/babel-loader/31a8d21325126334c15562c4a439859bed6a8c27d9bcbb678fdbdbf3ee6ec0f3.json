{"ast":null,"code":"import axios from 'axios';\nimport { ElMessage } from 'element-plus';\nimport { reactive, ref } from 'vue';\nexport default {\n  setup() {\n    let rankInfo = ref([]);\n    let time = ref();\n    let start = ref();\n    let end = ref();\n    function getInfo() {\n      axios.get('/url/rank', {\n        params: {\n          role: 'active'\n        }\n      }).then(res => res.data).then(res => {\n        if (res.code == 200) {\n          this.rankInfo = res.data;\n          // console.log(rankInfo);\n        } else {\n          ElMessage.error(\"获取排名列表失败\");\n        }\n      }).catch(e => {\n        ElMessage.error(\"获取排名列表失败\");\n      });\n    }\n    function sortByCF(x, y) {\n      return y.codeforces - x.codeforces;\n    }\n    function sortByLG(x, y) {\n      return y.luogu - x.luogu;\n    }\n    function sortByVJ(x, y) {\n      return y.vjudge - x.vjudge;\n    }\n    function sortByNK(x, y) {\n      return y.nowcoder - x.nowcoder;\n    }\n    function getTime() {\n      axios.get('/url/time').then(res => res.data).then(res => {\n        if (res.code == 200) this.timeInterval = res.data;\n        this.start = new Date(this.timeInterval[0]).toLocaleDateString();\n        this.end = new Date(this.timeInterval[1]).toLocaleDateString();\n      }).catch(e => {});\n    }\n    return {\n      rankInfo,\n      getInfo,\n      time,\n      getTime,\n      sortByCF,\n      sortByLG,\n      sortByVJ,\n      sortByNK,\n      start,\n      end\n    };\n  },\n  created() {\n    this.getInfo();\n    this.getTime();\n  }\n};","map":{"version":3,"names":["axios","ElMessage","reactive","ref","setup","rankInfo","time","start","end","getInfo","get","params","role","then","res","data","code","error","catch","e","sortByCF","x","y","codeforces","sortByLG","luogu","sortByVJ","vjudge","sortByNK","nowcoder","getTime","timeInterval","Date","toLocaleDateString","created"],"sources":["C:\\Users\\18725\\Desktop\\集训队管理系统开发\\manage\\front\\manage\\src\\components\\ActivePage.vue"],"sourcesContent":["<template>\r\n    <el-main class=\"main\">\r\n        <div class=\"mainBody\">\r\n            <div style=\"width: 80%;margin:0 auto;position:absolute;top:5%;left:10%;\">\r\n                <h1 style=\"text-align:center;width:100%;margin-bottom:20px\">现役成员排名</h1>\r\n                <div style=\"margin:10px auto; width:100%;text-align:center\">\r\n                    <span style=\"margin-left: 10px;\">排名计算区间：</span>\r\n                    <span>{{ start }} 到 </span>\r\n                    <span>{{ end }}</span>\r\n                </div>\r\n                <el-table :data=\"rankInfo\" style=\"width: 100%\">\r\n                    <el-table-column prop=\"ranking\" label=\"排名\" width=\"100\" />\r\n                    <el-table-column prop=\"username\" label=\"姓名\" width=\"100\" />\r\n                    <el-table-column prop=\"score\" label=\"总得分\" width=\"100\" />\r\n                    <el-table-column prop=\"codeforces\" label=\"codeforces\" sortable :sort-method=\"sortByCF\" />\r\n                    <el-table-column prop=\"luogu\" label=\"luogu\" sortable :sort-method=\"sortByLG\" />\r\n                    <el-table-column prop=\"vjudge\" label=\"vjudge\" sortable :sort-method=\"sortByVJ\" />\r\n                    <el-table-column prop=\"nowcoder\" label=\"nowcoder\" sortable :sort-method=\"sortByNK\" />\r\n                </el-table>\r\n            </div>\r\n        </div>\r\n    </el-main>\r\n</template>\r\n<script>\r\nimport axios from 'axios';\r\nimport { ElMessage } from 'element-plus';\r\nimport { reactive, ref } from 'vue';\r\n\r\n\r\nexport default {\r\n    setup() {\r\n        let rankInfo = ref([])\r\n        let time = ref();\r\n        let start = ref()\r\n        let end = ref()\r\n        function getInfo() {\r\n            axios.get('/url/rank', {\r\n                params: {\r\n                    role: 'active'\r\n                }\r\n            }).then(res => res.data)\r\n                .then(res => {\r\n                    if (res.code == 200) {\r\n                        this.rankInfo = res.data;\r\n                        // console.log(rankInfo);\r\n                    } else {\r\n                        ElMessage.error(\"获取排名列表失败\")\r\n                    }\r\n                }).catch(e => {\r\n                    ElMessage.error(\"获取排名列表失败\")\r\n                })\r\n        }\r\n        function sortByCF(x, y) {\r\n            return y.codeforces - x.codeforces;\r\n        }\r\n        function sortByLG(x, y) {\r\n            return y.luogu - x.luogu;\r\n        }\r\n        function sortByVJ(x, y) {\r\n            return y.vjudge - x.vjudge;\r\n        }\r\n        function sortByNK(x, y) {\r\n            return y.nowcoder - x.nowcoder;\r\n        }\r\n        function getTime() {\r\n            axios.get('/url/time')\r\n                .then(res => res.data)\r\n                .then(res => {\r\n                    if (res.code == 200)\r\n                        this.timeInterval = res.data;\r\n                    this.start = new Date(this.timeInterval[0]).toLocaleDateString();\r\n                    this.end = new Date(this.timeInterval[1]).toLocaleDateString();\r\n                })\r\n                .catch(e => { })\r\n        }\r\n        return {\r\n            rankInfo,\r\n            getInfo,\r\n            time,\r\n            getTime,\r\n            sortByCF,\r\n            sortByLG,\r\n            sortByVJ,\r\n            sortByNK,\r\n            start,\r\n            end\r\n        }\r\n    },\r\n    created() {\r\n        this.getInfo();\r\n        this.getTime();\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.main {\r\n    background-color: #EEE\r\n}\r\n\r\n.mainBody {\r\n    background-color: white;\r\n    width: 70%;\r\n    min-width: 700px;\r\n    height: 100%;\r\n    margin: 0 auto;\r\n    position: relative;\r\n    overflow: auto;\r\n}\r\n\r\n.myBody {\r\n    position: absolute;\r\n    left: 10%;\r\n    top: 10%;\r\n    width: 80%;\r\n    height: 90%;\r\n}\r\n</style>\r\n"],"mappings":"AAwBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,cAAc;AACxC,SAASC,QAAQ,EAAEC,GAAE,QAAS,KAAK;AAGnC,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,IAAIC,QAAO,GAAIF,GAAG,CAAC,EAAE;IACrB,IAAIG,IAAG,GAAIH,GAAG,CAAC,CAAC;IAChB,IAAII,KAAI,GAAIJ,GAAG,CAAC;IAChB,IAAIK,GAAE,GAAIL,GAAG,CAAC;IACd,SAASM,OAAOA,CAAA,EAAG;MACfT,KAAK,CAACU,GAAG,CAAC,WAAW,EAAE;QACnBC,MAAM,EAAE;UACJC,IAAI,EAAE;QACV;MACJ,CAAC,CAAC,CAACC,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,EAClBF,IAAI,CAACC,GAAE,IAAK;QACT,IAAIA,GAAG,CAACE,IAAG,IAAK,GAAG,EAAE;UACjB,IAAI,CAACX,QAAO,GAAIS,GAAG,CAACC,IAAI;UACxB;QACJ,OAAO;UACHd,SAAS,CAACgB,KAAK,CAAC,UAAU;QAC9B;MACJ,CAAC,CAAC,CAACC,KAAK,CAACC,CAAA,IAAK;QACVlB,SAAS,CAACgB,KAAK,CAAC,UAAU;MAC9B,CAAC;IACT;IACA,SAASG,QAAQA,CAACC,CAAC,EAAEC,CAAC,EAAE;MACpB,OAAOA,CAAC,CAACC,UAAS,GAAIF,CAAC,CAACE,UAAU;IACtC;IACA,SAASC,QAAQA,CAACH,CAAC,EAAEC,CAAC,EAAE;MACpB,OAAOA,CAAC,CAACG,KAAI,GAAIJ,CAAC,CAACI,KAAK;IAC5B;IACA,SAASC,QAAQA,CAACL,CAAC,EAAEC,CAAC,EAAE;MACpB,OAAOA,CAAC,CAACK,MAAK,GAAIN,CAAC,CAACM,MAAM;IAC9B;IACA,SAASC,QAAQA,CAACP,CAAC,EAAEC,CAAC,EAAE;MACpB,OAAOA,CAAC,CAACO,QAAO,GAAIR,CAAC,CAACQ,QAAQ;IAClC;IACA,SAASC,OAAOA,CAAA,EAAG;MACf9B,KAAK,CAACU,GAAG,CAAC,WAAW,EAChBG,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,EACpBF,IAAI,CAACC,GAAE,IAAK;QACT,IAAIA,GAAG,CAACE,IAAG,IAAK,GAAG,EACf,IAAI,CAACe,YAAW,GAAIjB,GAAG,CAACC,IAAI;QAChC,IAAI,CAACR,KAAI,GAAI,IAAIyB,IAAI,CAAC,IAAI,CAACD,YAAY,CAAC,CAAC,CAAC,CAAC,CAACE,kBAAkB,CAAC,CAAC;QAChE,IAAI,CAACzB,GAAE,GAAI,IAAIwB,IAAI,CAAC,IAAI,CAACD,YAAY,CAAC,CAAC,CAAC,CAAC,CAACE,kBAAkB,CAAC,CAAC;MAClE,CAAC,EACAf,KAAK,CAACC,CAAA,IAAK,CAAE,CAAC;IACvB;IACA,OAAO;MACHd,QAAQ;MACRI,OAAO;MACPH,IAAI;MACJwB,OAAO;MACPV,QAAQ;MACRI,QAAQ;MACRE,QAAQ;MACRE,QAAQ;MACRrB,KAAK;MACLC;IACJ;EACJ,CAAC;EACD0B,OAAOA,CAAA,EAAG;IACN,IAAI,CAACzB,OAAO,CAAC,CAAC;IACd,IAAI,CAACqB,OAAO,CAAC,CAAC;EAClB;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}