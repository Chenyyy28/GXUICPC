{"ast":null,"code":"// import { resolve } from 'core-js/fn/promise';\nimport { ref, reactive } from 'vue';\nexport default {\n  setup() {\n    let isCollapse = ref(true);\n    let asideWidth = ref(70);\n    let asideState = reactive({\n      routes: []\n    });\n    let activePath = ref('1');\n    function close() {\n      // asideState.routes;\n      asideWidth.value = 70;\n      console.log(asideWidth);\n    }\n    function open() {\n      asideWidth.value = 200;\n    }\n    function getRoutes() {\n      asideState.routes = JSON.parse(localStorage.getItem('routeLists'));\n    }\n    function changeActivePath(id) {\n      localStorage.setItem('active', id);\n    }\n    return {\n      isCollapse,\n      asideWidth,\n      asideState,\n      open,\n      close,\n      // getMenu,\n      getRoutes,\n      activePath,\n      changeActivePath\n    };\n  },\n  beforeCreate: function () {\n    this.getRoutes();\n    if (localStorage.getItem('active') !== null) this.activePath = localStorage.getItem('active');else this.activePath = '1';\n  }\n};","map":{"version":3,"names":["ref","reactive","setup","isCollapse","asideWidth","asideState","routes","activePath","close","value","console","log","open","getRoutes","JSON","parse","localStorage","getItem","changeActivePath","id","setItem","beforeCreate"],"sources":["C:\\Users\\18725\\Desktop\\集训队管理系统开发\\manage\\front\\manage\\src\\components\\Aside.vue"],"sourcesContent":["<template>\r\n\r\n    <el-aside :style=\"{ width: asideWidth + 'px' }\" style=\"overflow: hidden;\" class=\"aside\">\r\n        <el-radio-group v-model=\"isCollapse\" style=\"margin-bottom: 10px;width: 70px; overflow: hidden;\">\r\n            <!-- <el-radio-button @click=\"open\" v-show=\"isCollapse\" :value=\"false\">\r\n                <el-icon>\r\n                    <Open />\r\n                </el-icon>\r\n            </el-radio-button>\r\n            <el-radio-button @click=\"close\" v-show=\"!isCollapse\" :value=\"true\">\r\n                <el-icon>\r\n                    <TurnOff />\r\n                </el-icon>\r\n            </el-radio-button> -->\r\n        </el-radio-group>\r\n        <el-menu :default-active=\"activePath\" active-text-color=\"#409EFF\" :collapse=\"isCollapse\" @open=\"open\"\r\n            @close=\"close\" :collapse-transition='false' style=\"width: 100%;height: 100%;border:0;\">\r\n            <!-- <p>YEs</p> -->\r\n            <template v-for=\"item in asideState.routes\" :key=\"item.id\">\r\n                <router-link :to=\"item.link\">\r\n                    <el-menu-item @click=\"changeActivePath(item.id)\" :index=\"item.id + ''\">\r\n                        <el-icon>\r\n                            <component :is=\"item.icon\" />\r\n                        </el-icon>\r\n                        <template #title>{{ item.name }}</template>\r\n                    </el-menu-item>\r\n                </router-link>\r\n\r\n            </template>\r\n        </el-menu>\r\n    </el-aside>\r\n</template>\r\n<script>\r\n// import { resolve } from 'core-js/fn/promise';\r\nimport { ref, reactive } from 'vue'\r\nexport default {\r\n    setup() {\r\n        let isCollapse = ref(true);\r\n        let asideWidth = ref(70);\r\n        let asideState = reactive({\r\n            routes: []\r\n        });\r\n        let activePath = ref('1');\r\n        function close() {\r\n            // asideState.routes;\r\n            asideWidth.value = 70;\r\n            console.log(asideWidth)\r\n        }\r\n        function open() {\r\n            asideWidth.value = 200;\r\n        }\r\n        function getRoutes() {\r\n            asideState.routes = JSON.parse(localStorage.getItem('routeLists'));\r\n        }\r\n        function changeActivePath(id) {\r\n            localStorage.setItem('active', id);\r\n        }\r\n        return {\r\n            isCollapse,\r\n            asideWidth,\r\n            asideState,\r\n            open,\r\n            close,\r\n            // getMenu,\r\n            getRoutes,\r\n            activePath,\r\n            changeActivePath\r\n        }\r\n    },\r\n    beforeCreate: function () {\r\n        this.getRoutes();\r\n\r\n        if (localStorage.getItem('active') !== null)\r\n            this.activePath = localStorage.getItem('active');\r\n        else this.activePath = '1';\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n.aside {\r\n    /* background-color: #a1a3a6; */\r\n}\r\n\r\n/* .el-menu-vertical-demo:not(.el-menu--collapse) {\r\n    width: 260px;\r\n    height: 100%;\r\n\r\n} */\r\n/* .el-menu-item.is-active {\r\n  background-color: #409eff!important;\r\n} */\r\n</style>"],"mappings":"AAiCA;AACA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,IAAIC,UAAS,GAAIH,GAAG,CAAC,IAAI,CAAC;IAC1B,IAAII,UAAS,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACxB,IAAIK,UAAS,GAAIJ,QAAQ,CAAC;MACtBK,MAAM,EAAE;IACZ,CAAC,CAAC;IACF,IAAIC,UAAS,GAAIP,GAAG,CAAC,GAAG,CAAC;IACzB,SAASQ,KAAKA,CAAA,EAAG;MACb;MACAJ,UAAU,CAACK,KAAI,GAAI,EAAE;MACrBC,OAAO,CAACC,GAAG,CAACP,UAAU;IAC1B;IACA,SAASQ,IAAIA,CAAA,EAAG;MACZR,UAAU,CAACK,KAAI,GAAI,GAAG;IAC1B;IACA,SAASI,SAASA,CAAA,EAAG;MACjBR,UAAU,CAACC,MAAK,GAAIQ,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,YAAY,CAAC,CAAC;IACtE;IACA,SAASC,gBAAgBA,CAACC,EAAE,EAAE;MAC1BH,YAAY,CAACI,OAAO,CAAC,QAAQ,EAAED,EAAE,CAAC;IACtC;IACA,OAAO;MACHhB,UAAU;MACVC,UAAU;MACVC,UAAU;MACVO,IAAI;MACJJ,KAAK;MACL;MACAK,SAAS;MACTN,UAAU;MACVW;IACJ;EACJ,CAAC;EACDG,YAAY,EAAE,SAAAA,CAAA,EAAY;IACtB,IAAI,CAACR,SAAS,CAAC,CAAC;IAEhB,IAAIG,YAAY,CAACC,OAAO,CAAC,QAAQ,MAAM,IAAI,EACvC,IAAI,CAACV,UAAS,GAAIS,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC,MAC/C,IAAI,CAACV,UAAS,GAAI,GAAG;EAC9B;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}