{"ast":null,"code":"import router from '@/router';\nimport store from '@/store';\nimport axios from 'axios';\nimport { ElMessage } from 'element-plus';\nimport { ref, reactive } from 'vue';\nexport default {\n  setup() {\n    let isCollapse = ref(true);\n    let asideWidth = ref(70);\n    let asideState = reactive({\n      routes: []\n    });\n    function close() {\n      asideState.routes;\n      console.log(asideWidth);\n    }\n    function open() {\n      asideWidth.value = 200;\n    }\n    // function addRoutes(lists) {\n    //     lists.array.forEach(route => {\n    //         router.addRoute(\n    //             \"Main\",\n    //             {\n    //                 path: route.link,\n    //                 component: route.pageName,\n    //                 name: route.pageName\n    //             }\n    //         )\n    //     });\n    // }\n    function dynamicRouter() {\n      // this.$store.commit('setDynamicRouter', JSON.stringify(lists));\n      axios.get(\"/url/route\").then(res => res.data).then(data => {\n        const route = JSON.stringify(data);\n        const lists = JSON.parse(route).data;\n        store.commit('setDynamicRouter', route);\n        asideState.routes = lists;\n        console.log(asideState.routes);\n        // addRoutes(lists);\n      }).catch(e => {\n        console.log(e);\n        ElMessage.error(\"菜单请求错误\");\n      });\n    }\n    return {\n      isCollapse,\n      asideWidth,\n      asideState,\n      open,\n      close,\n      dynamicRouter\n    };\n  },\n  created: function () {\n    this.dynamicRouter();\n  }\n};","map":{"version":3,"names":["router","store","axios","ElMessage","ref","reactive","setup","isCollapse","asideWidth","asideState","routes","close","console","log","open","value","dynamicRouter","get","then","res","data","route","JSON","stringify","lists","parse","commit","catch","e","error","created"],"sources":["C:\\Users\\18725\\Desktop\\集训队管理系统开发\\manage\\front\\manage\\src\\components\\Aside.vue"],"sourcesContent":["<template>\r\n\r\n    <el-aside :style=\"{ width: asideWidth + 'px' }\" style=\"overflow: hidden;\" class=\"aside\">\r\n        <el-radio-group v-model=\"isCollapse\" style=\"margin-bottom: 10px;width: 70px; overflow: hidden;\">\r\n            <el-radio-button @click=\"open\" v-show=\"isCollapse\" :value=\"false\">\r\n                <el-icon>\r\n                    <Open />\r\n                </el-icon>\r\n            </el-radio-button>\r\n            <el-radio-button @click=\"close\" v-show=\"!isCollapse\" :value=\"true\">\r\n                <el-icon>\r\n                    <TurnOff />\r\n                </el-icon>\r\n            </el-radio-button>\r\n        </el-radio-group>\r\n        <el-menu default-active=\"1\" active-text-color=\"#409EFF\" :collapse=\"isCollapse\" @open=\"open\" @close=\"close\"\r\n            :collapse-transition='false' style=\"width: 100%;height: 100%;border:0;\">\r\n            <!-- <p>YEs</p> -->\r\n            <template v-for=\"item in asideState.routes\" :key=\"item.id\">\r\n                <router-link :to=\"item.link\">\r\n                    <el-menu-item :index=\"item.id + ''\">\r\n                        <el-icon>\r\n                            <component :is=\"item.icon\" />\r\n                        </el-icon>\r\n                        <template #title>{{ item.name }}</template>\r\n                    </el-menu-item>\r\n                </router-link>\r\n\r\n            </template>\r\n        </el-menu>\r\n    </el-aside>\r\n</template>\r\n<script>\r\nimport router from '@/router';\r\nimport store from '@/store';\r\nimport axios from 'axios';\r\nimport { ElMessage } from 'element-plus';\r\nimport { ref, reactive } from 'vue'\r\nexport default {\r\n    setup() {\r\n        let isCollapse = ref(true);\r\n        let asideWidth = ref(70);\r\n        let asideState = reactive({\r\n            routes: []\r\n        });\r\n        function close() {\r\n            asideState.routes;\r\n            console.log(asideWidth)\r\n        }\r\n        function open() {\r\n            asideWidth.value = 200\r\n        }\r\n        // function addRoutes(lists) {\r\n        //     lists.array.forEach(route => {\r\n        //         router.addRoute(\r\n        //             \"Main\",\r\n        //             {\r\n        //                 path: route.link,\r\n        //                 component: route.pageName,\r\n        //                 name: route.pageName\r\n        //             }\r\n        //         )\r\n        //     });\r\n        // }\r\n        function dynamicRouter() {\r\n            // this.$store.commit('setDynamicRouter', JSON.stringify(lists));\r\n            axios.get(\"/url/route\").then(res => res.data).then(data => {\r\n                const route = JSON.stringify(data);\r\n                const lists = JSON.parse(route).data;\r\n                store.commit('setDynamicRouter', route)\r\n                asideState.routes = lists;\r\n                console.log(asideState.routes);\r\n                // addRoutes(lists);\r\n            }).catch(e => {\r\n                console.log(e);\r\n                ElMessage.error(\"菜单请求错误\");\r\n            })\r\n\r\n        }\r\n        return {\r\n            isCollapse,\r\n            asideWidth,\r\n            asideState,\r\n            open,\r\n            close,\r\n            dynamicRouter\r\n        }\r\n    },\r\n    created: function () {\r\n        this.dynamicRouter();\r\n    }\r\n}\r\n</script>\r\n<style>\r\n.aside {\r\n    /* background-color: #a1a3a6; */\r\n}\r\n\r\n/* .el-menu-vertical-demo:not(.el-menu--collapse) {\r\n    width: 260px;\r\n    height: 100%;\r\n\r\n} */\r\n/* .el-menu-item.is-active {\r\n  background-color: #409eff!important;\r\n} */\r\n</style>"],"mappings":"AAiCA,OAAOA,MAAK,MAAO,UAAU;AAC7B,OAAOC,KAAI,MAAO,SAAS;AAC3B,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,cAAc;AACxC,SAASC,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,IAAIC,UAAS,GAAIH,GAAG,CAAC,IAAI,CAAC;IAC1B,IAAII,UAAS,