{"ast":null,"code":"import router from '@/router';\nimport axios from 'axios';\nimport { ElMessage } from 'element-plus';\n// import { resolve } from 'core-js/fn/promise';\nimport { ref, reactive } from 'vue';\nexport default {\n  setup() {\n    let isCollapse = ref(true);\n    let asideWidth = ref(70);\n    let asideState = reactive({\n      routes: []\n    });\n    function close() {\n      // asideState.routes;\n      asideWidth.value = 70;\n      console.log(asideWidth);\n    }\n    function open() {\n      asideWidth.value = 200;\n    }\n    function getRoutes() {\n      axios.get(\"/url/route\").then(res => res.data).then(data => {\n        return data.data;\n      }).then(data => {\n        // getMenu();\n        asideState.routes = data;\n        data.forEach(route => {\n          console.log(route);\n          router.addRoute(\"MainPage\", {\n            path: route.link,\n            name: route.pageName,\n            component: () => import('@/components/' + route.pageName)\n          });\n        });\n        // {\n        //     path: '/home',\n        //         name: 'HomePage',\n        //             component: HomePage\n        // }\n      }).catch(e => {\n        console.log(e);\n        ElMessage.error(\"菜单请求错误\");\n      });\n    }\n    return {\n      isCollapse,\n      asideWidth,\n      asideState,\n      open,\n      close,\n      // getMenu,\n      getRoutes\n    };\n  },\n  created: function () {\n    this.getRoutes();\n  }\n};","map":{"version":3,"names":["router","axios","ElMessage","ref","reactive","setup","isCollapse","asideWidth","asideState","routes","close","value","console","log","open","getRoutes","get","then","res","data","forEach","route","addRoute","path","link","name","pageName","component","catch","e","error","created"],"sources":["C:\\Users\\18725\\Desktop\\集训队管理系统开发\\manage\\front\\manage\\src\\components\\Aside.vue"],"sourcesContent":["<template>\r\n\r\n    <el-aside :style=\"{ width: asideWidth + 'px' }\" style=\"overflow: hidden;\" class=\"aside\">\r\n        <el-radio-group v-model=\"isCollapse\" style=\"margin-bottom: 10px;width: 70px; overflow: hidden;\">\r\n            <el-radio-button @click=\"open\" v-show=\"isCollapse\" :value=\"false\">\r\n                <el-icon>\r\n                    <Open />\r\n                </el-icon>\r\n            </el-radio-button>\r\n            <el-radio-button @click=\"close\" v-show=\"!isCollapse\" :value=\"true\">\r\n                <el-icon>\r\n                    <TurnOff />\r\n                </el-icon>\r\n            </el-radio-button>\r\n        </el-radio-group>\r\n        <el-menu default-active=\"1\" active-text-color=\"#409EFF\" :collapse=\"isCollapse\" @open=\"open\" @close=\"close\"\r\n            :collapse-transition='false' style=\"width: 100%;height: 100%;border:0;\">\r\n            <!-- <p>YEs</p> -->\r\n            <template v-for=\"item in asideState.routes\" :key=\"item.id\">\r\n                <router-link :to=\"item.link\">\r\n                    <el-menu-item :index=\"item.id + ''\">\r\n                        <el-icon>\r\n                            <component :is=\"item.icon\" />\r\n                        </el-icon>\r\n                        <template #title>{{ item.name }}</template>\r\n                    </el-menu-item>\r\n                </router-link>\r\n\r\n            </template>\r\n        </el-menu>\r\n    </el-aside>\r\n</template>\r\n<script>\r\n\r\nimport router from '@/router';\r\nimport axios from 'axios';\r\nimport { ElMessage } from 'element-plus';\r\n// import { resolve } from 'core-js/fn/promise';\r\nimport { ref, reactive } from 'vue'\r\nexport default {\r\n    setup() {\r\n        let isCollapse = ref(true);\r\n        let asideWidth = ref(70);\r\n        let asideState = reactive({\r\n            routes: []\r\n        });\r\n        function close() {\r\n            // asideState.routes;\r\n            asideWidth.value = 70;\r\n            console.log(asideWidth)\r\n        }\r\n        function open() {\r\n            asideWidth.value = 200;\r\n        }\r\n        function getRoutes() {\r\n            axios.get(\"/url/route\").then(res => res.data).then(data => {\r\n                return data.data;\r\n            }).then((data) => {\r\n                // getMenu();\r\n                asideState.routes = data;\r\n                data.forEach(route => {\r\n                    console.log(route);\r\n                    \r\n                    router.addRoute(\"MainPage\", {\r\n                        path: route.link,\r\n                        name: route.pageName,\r\n                        component: () => import('@/components/' + route.pageName)\r\n                    })\r\n                });\r\n                // {\r\n                //     path: '/home',\r\n                //         name: 'HomePage',\r\n                //             component: HomePage\r\n                // }\r\n            }).catch(e => {\r\n                console.log(e);\r\n                ElMessage.error(\"菜单请求错误\");\r\n            })\r\n        }\r\n        return {\r\n            isCollapse,\r\n            asideWidth,\r\n            asideState,\r\n            open,\r\n            close,\r\n            // getMenu,\r\n            getRoutes\r\n        }\r\n    },\r\n    created: function () {\r\n        this.getRoutes();\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n.aside {\r\n    /* background-color: #a1a3a6; */\r\n}\r\n\r\n/* .el-menu-vertical-demo:not(.el-menu--collapse) {\r\n    width: 260px;\r\n    height: 100%;\r\n\r\n} */\r\n/* .el-menu-item.is-active {\r\n  background-color: #409eff!important;\r\n} */\r\n</style>"],"mappings":"AAkCA,OAAOA,MAAK,MAAO,UAAU;AAC7B,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,cAAc;AACxC;AACA,SAASC,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,IAAIC,UAAS,GAAIH,GAAG,CAAC,IAAI,CAAC;IAC1B,IAAII,UAAS,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACxB,IAAIK,UAAS,GAAIJ,QAAQ,CAAC;MACtBK,MAAM,EAAE;IACZ,CAAC,CAAC;IACF,SAASC,KAAKA,CAAA,EAAG;MACb;MACAH,UA